{
  "$id": "places.schema.json",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["name", "formatted_address", "lat", "lng"],
    "properties": {
      "name": { "type": "string", "minLength": 1 },
      "formatted_address": { "type": "string" },
      "lat": { "type": "number" },
      "lng": { "type": "number" }
    },
    "additionalProperties": true
  }
}
