{
  "$id": "weather.schema.json",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["date", "hi", "lo", "precip"],
    "properties": {
      "date": { "type": "string", "format": "date" },
      "hi": { "type": "number" },
      "lo": { "type": "number" },
      "precip": { "type": "number" }
    },
    "additionalProperties": false
  }
}
